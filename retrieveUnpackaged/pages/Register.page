<apex:page standardController="User__c" id="new">
<apex:form >
<apex:pageBlock >
<apex:pageBlockSection title="Registeration">
<apex:inputField value="{!User__c.First_Name__c}"/>
<apex:inputField value="{!User__c.Last_Name__c}"/>
<apex:inputField value="{!User__c.Email_Id__c}"/>
<apex:inputField value="{!User__c.Address__c}"/>
<apex:inputField value="{!User__c.Phone_Number__c}"/>
<apex:inputField value="{!User__c.State__c}"/>
<apex:inputField value="{!User__c.User_Name__c}"/>
<apex:inputField value="{!User__c.Password__c}"/>
</apex:pageBlockSection>
<apex:pageBlockButtons ><apex:commandButton value="Save" action="{!save}" reRender="new"/><apex:commandButton value="Cancel" action="{!cancel}" reRender="new"/></apex:pageBlockButtons>

</apex:pageBlock>
</apex:form>
</apex:page>